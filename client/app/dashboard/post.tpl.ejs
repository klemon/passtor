<section>
	<div role="main" class="main">

		<div class="container">

			<div class="row">
				<div class="col-md-9">
					<div class="blog-posts">

						<article class="post post-medium">
							<div class="row">

								<div class="col-md-5">
									<div class="post-image single">
										<img class="img-thumbnail" src="public/img/blog-medium-image-2.jpg" alt="">
									</div>
								</div>
								<div class="col-md-7">

									<div class="post-content">

										<h2><a href="">{{post.title}}</a></h2>
										<p>{{post.description}}</p>

									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-12">
									<div class="post-meta">
										<span><i class="icon icon-calendar"></i>{{post.created}}</span>
										<span><i class="icon icon-user"></i> By <a href="#">{{post.creator}}</a> </span>
										<span><i class="icon icon-tag"></i> <a href="#">Duis</a>, <a href="#">News</a> </span>
										<span><i class="icon icon-comments"></i> <a href="#">12 Comments</a></span>
										<a href="" class="btn btn-xs btn-primary pull-right">Read more...</a>
									</div>
								</div>
							</div>
						</article>

					</div>
				</div>
			</div>

		</div>

	</div>
</section>